- var cards = [{name: 'semf ucuk',place: 'ceo & founder'},{name: 'dik adalin', place: 'engineering'},{name: 'jeng kol', place: 'designer'},{name: 'pet romak', place: 'marketing'}]

mixin list(items)
  each item in items
    .member.col-6.col-md-4.col-lg-3.mb-md-4
      .card.bg-transparent
        svg.bd-placeholder-img.card-img-top.rounded-0(width="100%" height="225" xmlns="http://www.w3.org/2000/svg" role="img" aria-label="Placeholder: Thumbnail" preserveAspectRatio="xMidYMid slice" focusable="false")
          title Placeholder
          rect(width="100%" height="100%" fill="#ffffff")
        .card-body.member__body
          h4.member__name= item.name
          p.member__position= item.place

+list(cards)
